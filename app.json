        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.anonymous.pharmacontrol"
    },
    "web": {
      "favicon": "./assets/logo1.png"
    },
    "extra": {
      "eas": {
        "projectId": "18502d9b-4625-4d3a-8787-f9b96b300691"
